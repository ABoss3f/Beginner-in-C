/Receive two numbers, calculate and show the subtraction of the first by the second/

#include<stdio.h>
#include<locale.h>

int main(){
	
	setlocale(LC_ALL,"portuguese");
	float num1;
	float num2;
	
	
	printf("Digite o primeiro número: \n");
	scanf("%f", &num1);
	
	printf("Digite o segundo número: \n");
	scanf("%f", &num2);
	
	float result = num1 - num2;
	printf("O resultado  será = %f", result);
	
}
